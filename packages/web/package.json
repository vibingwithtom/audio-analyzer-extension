{
  "name": "@audio-analyzer/web",
  "version": "1.0.0",
  "description": "Web-based audio analyzer application",
  "homepage": "https://audio-analyzer.tinytech.site",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:beta": "vite build --mode beta",
    "preview": "vite preview",
    "serve": "serve dist",
    "deploy": "npm run build && echo audio-analyzer.tinytech.site > dist/CNAME && gh-pages -d dist --add",
    "deploy:beta": "npm run build:beta && gh-pages -d dist -e beta --add",
    "deploy:clean": "npm run build && echo audio-analyzer.tinytech.site > dist/CNAME && gh-pages -d dist",
    "deploy:beta:clean": "npm run build:beta && gh-pages -d dist -e beta",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@audio-analyzer/core": "^1.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.18.9",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "jsdom": "^27.0.0",
    "serve": "^14.2.0",
    "svelte": "^5.39.11",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "web",
    "audio",
    "analyzer",
    "progressive-web-app"
  ],
  "author": "vibingwithtom",
  "license": "MIT"
}
